<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/background"
    >
    <RelativeLayout
        android:id="@+id/gameboard"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
       >
        <RelativeLayout
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_centerInParent="true"
            android:id="@+id/button1"
            android:layout_toLeftOf="@id/button5"
            android:layout_marginRight="3dp"
            android:layout_above="@id/button5"
            android:background="@color/black"
            android:layout_marginBottom="22dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/background"
                android:background="@color/background"
                android:id="@+id/buttonImage1">


            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_centerInParent="true"
            android:id="@+id/button2"
            android:layout_above="@id/button5"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:background="@color/black"
            android:layout_marginBottom="22dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/background"
                android:background="@color/background"
                android:id="@+id/buttonImage2">


            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_centerInParent="true"
            android:id="@+id/button3"
            android:layout_toRightOf="@id/button2"
            android:layout_above="@id/button5"
            android:layout_marginLeft="3dp"
            android:background="@color/black"
            android:layout_marginBottom="22dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/background"
                android:background="@color/background"
                android:id="@+id/buttonImage3">


            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_centerInParent="true"
            android:id="@+id/button4"
            android:layout_toLeftOf="@id/button5"
            android:layout_marginRight="3dp"
            android:background="@color/black"
            android:layout_marginTop="22dp">

            <ImageView
                android:id="@+id/buttonImage4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/background"
                android:background="@color/background">


            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_centerInParent="true"
            android:id="@+id/button5"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:background="@color/black">

            <ImageView
                android:id="@+id/buttonImage5"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/background"
                android:background="@color/background">


            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_centerInParent="true"
            android:id="@+id/button6"
            android:layout_toRightOf="@id/button5"
            android:layout_marginLeft="3dp"
            android:background="@color/black"
            android:layout_marginTop="22dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/background"
                android:background="@color/background"
                android:id="@+id/buttonImage6">


            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_centerInParent="true"
            android:id="@+id/button7"
            android:layout_toLeftOf="@id/button8"
            android:layout_below="@id/button5"
            android:layout_marginRight="3dp"
            android:background="@color/black"
            android:layout_marginTop="22dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/background"
                android:background="@color/background"
                android:id="@+id/buttonImage7">


            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_centerInParent="true"
            android:id="@+id/button8"
            android:layout_below="@id/button5"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:background="@color/black"
            android:layout_marginTop="22dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/background"
                android:background="@color/background"
                android:id="@+id/buttonImage8">


            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_centerInParent="true"
            android:id="@+id/button9"
            android:layout_toRightOf="@id/button8"
            android:layout_marginLeft="3dp"
            android:layout_below="@id/button5"
            android:background="@color/black"
            android:layout_marginTop="22dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/background"
                android:background="@color/background"
                android:id="@+id/buttonImage9">


            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="330dp"
            android:layout_height="8dp"
            android:background="@color/blank"
            android:layout_centerHorizontal="true"
            android:layout_above="@id/button5"
            android:layout_marginBottom="8dp">


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="330dp"
            android:layout_height="8dp"
            android:background="@color/blank"
            android:layout_centerHorizontal="true"
            android:layout_below="@id/button5"
            android:layout_marginTop="8dp">


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="8dp"
            android:layout_height="330dp"
            android:background="@color/blank"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/button5"
            android:layout_marginRight="-12dp">


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="8dp"
            android:layout_height="330dp"
            android:background="@color/blank"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@id/button5"
            android:layout_marginLeft="-12dp">


        </RelativeLayout>

</RelativeLayout>
</RelativeLayout>